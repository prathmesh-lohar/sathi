WSGIScriptAlias / /var/www/html/sathi/sathi/wsgi.py
WSGIPythonPath /var/www/html/sathi/
Alias /static/ /var/www/html/sathi/static/
<Directory /var/www/html/sathi/>
   <Files wsgi.py>
      Order deny,allow
      Allow from all
   </Files>
</Directory>


<Directory /var/www/html/sathi/static>
    Options Indexes FollowSymLinks
    AllowOverride None
    Require all granted
</Directory>


Alias /media/ /var/www/html/sathi/media/
<Directory /var/www/html/sathi/media/>
    Require all granted
</Directory>
